   <body>
      @{
        var commonData = Context.Items["CommonData"] as LayoutViewModel;
      }
      <!-- ***** Preloader Start ***** -->
      <div id="preloader">
         <div class="jumper">
            <div></div>
            <div></div>
            <div></div>
         </div>
      </div>
      <!-- ***** Preloader End ***** -->
      <!-- Header -->
      <header class="">
         <nav class="navbar navbar-expand-lg">
            <div class="container">
               <a class="navbar-brand" href='@Url.Action("Index", "Home")'>
                  <h2>พีเอส <em>ไม้อัดไทย</em></h2>
               </a>
               <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
               <span class="navbar-toggler-icon"></span>
               </button>
               <div class="collapse navbar-collapse" id="navbarResponsive">
                  <ul class="navbar-nav ml-auto">
                    <li class="nav-item @(controllerName == "Home" ? "active": "")">
                        <a class="nav-link" href='@Url.Action("Index", "Home")'>@commonData?.Setting?.menuHome
                            <span class="sr-only">(current)</span>
                        </a>
                     </li>
                    <li class="nav-item @(controllerName == "Product" ? "active": "")">
                        <a class="nav-link" href='@Url.Action("Index", "Product")'>@commonData?.Setting?.menuProduct</a>
                     </li>
                        <li class="nav-item @(controllerName == "ContactUs" ? "active": "")">
                        <a class="nav-link" href='@Url.Action("Index", "ContactUs")'>@commonData?.Setting?.menuContactUs</a>
                     </li>
                     <li class="nav-item @(controllerName == "Article" ? "active": "")">
                        <a class="nav-link" href='@Url.Action("Index", "Article")'>@commonData?.Setting?.menuContent</a>
                     </li>
                  </ul>
               </div>
            </div>
         </nav>
      </header>


      @RenderBody()


      <footer class="footer mt-5">
         <div class="container bottom_border">
            <div class="row">
               <div class=" col-sm-4 col-md col-sm-4  col-12 col">
                  <h5 class="headin5_amrc col_white_amrc pt2">Find us </h5>
                  <p class="mb10">@commonData?.Contact?.Paragraph1</p>
                  <p><i class="fa fa-location-arrow"></i> @commonData?.Contact?.Latitude </p>
                    <p><i class="fa fa-phone"></i><a href="tel:@commonData?.Contact?.Phone">  @commonData?.Contact?.Phone </a> </p>
                    <p><i class="fa fa-phone"></i> <a href="tel:@commonData?.Contact?.Paragraph2"> @commonData?.Contact?.Paragraph2 </a>  </p>
                    <p><i class="fa fa fa-envelope"></i> <a href="mailto:@commonData?.Contact?.Instagram">@commonData?.Contact?.Instagram </a> </p>
               </div>
               <div class=" col-sm-4 col-md  col-6 col">
                  <h5 class="headin5_amrc col_white_amrc pt2">Line</h5>
                  <a href="@commonData?.Contact?.Line2">
                    <img width="150" src="@commonData?.Contact?.Paragraph3">
                  </a>
               </div>
               <div class=" col-sm-4 col-md  col-6 col">
                  <h5 class="headin5_amrc col_white_amrc pt2">Online Status</h5>
                  <ul class="footer_ul_amrc">
                     <li>
                        @*<p><b id="online_user">💚</b> Curent user : 20</p>*@
                        <p><img src="~/psplywood/assets/images/online.gif" /> User Online : @commonData?.UserOnlineCnt </p>
                     </li>
                     <li>
                        <p><i class="fa fa-users"></i> All User Visit : @commonData?.SiteVisitCounter</p>
                     </li>
                     <li>
                        <p><i class="fa fa-calendar"></i>: @DateTime.Now.ToString("dd/MM/yyyy")</p>
                     </li>
                  </ul>
               </div>
               <div class=" col-sm-4 col-md  col-12 col">
                  <h5 class="headin5_amrc col_white_amrc pt2">Map</h5>
                  <div class="map-container">
                     <iframe src="@commonData?.Contact?.Longitude" width="400" height="250" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                  </div>
               </div>
            </div>
         </div>
         <div class="container">
            <ul class="foote_bottom_ul_amrc">
